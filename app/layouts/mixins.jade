mixin launchElevate
    section.marketing-options.text-left.marketing-options-white
      .container
        .row
          .col-xs-12
              .row.row-flex
                .col-flex
                  a(href="https://launch.smartup.io/discover", target="_blank")
                    img.img-responsive(src='/images/launch_bright.png')
                  .marketing-option-info.marketing-option-info-home
                    .marketing-option-info-home-wrap
                        h4 <a href="https://launch.smartup.io/discover">Launch</a>
                        p
                          | The startup community for entrepreneurs and innovators everywhere. 
                          | Test and improve your startup knowledge with insights and qiuzzes from top founders, 
                          | investors and journalists Founders is the original SmartUp community.
                          
                .col-flex
                  a(href="https://elevate.smartup.io/community", target="_blank")
                    img.img-responsive(src='/images/elevate_bright.png')
                  .marketing-option-info.marketing-option-info-home
                      .marketing-option-info-home-wrap
                        h4 <a href="https://elevate.smartup.io/community">Elevate</a>
                        p
                          | The innovation community for visionaries and mavericks 
                          | in mature businesses. Find the tools and insights that 
                          | will help you innovate regardless of where you work 
                          | and what you do.
                          
                          
mixin carouselItem-1
  .item.active
    .carousel-row-wrap
      .row-flex
        .col-flex.carousel-col-big
          a(href="https://launch.smartup.io/discover", target="_blank")
            img.img-responsive(src='/images/imgpsh_fullsize (6)_l.png')
          .marketing-option-info.marketing-option-info-home
            .marketing-option-info-home-wrap
              h4 <a href="https://launch.smartup.io/discover", target="_blank">Launch</a>
              p.text-justify
                | The startup community for entrepreneurs and innovators everywhere. 
                | Test and improve your startup knowledge with insights and qiuzzes from top founders, 
                | investors and journalists Founders is the original SmartUp community.
        .col-flex.col-flex-4
          .col-flex.margin-bottom-30
            a(href="https://launch.smartup.io/content/668", target='_blank')
              img.img-responsive(src='/images/imgpsh_fullsize (7)_l.png')
